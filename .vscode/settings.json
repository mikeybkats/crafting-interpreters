{
  "rust-analyzer.linkedProjects": [
    "rust_lox/Cargo.toml"
  ],
  "eslint.enable": false,
  "files.associations": {
    "stdint.h": "c",
    "stdbool.h": "c",
    "stddef.h": "c",
    "__bit_reference": "c",
    "__hash_table": "c",
    "__split_buffer": "c",
    "__tree": "c",
    "array": "c",
    "iterator": "c",
    "map": "c",
    "set": "c",
    "string": "c",
    "string_view": "c",
    "unordered_map": "c",
    "unordered_set": "c",
    "vector": "c",
    "typeinfo": "c",
    "chunk.h": "c",
    "common.h": "c",
    "debug.h": "c",
    "unity.h": "c",
    "__tuple": "c",
    "type_traits": "c",
    "limits": "c",
    "ratio": "c",
    "tuple": "c",
    "variant": "c",
    "chrono": "c",
    "compare": "c",
    "cstddef": "c",
    "test_chunk.h": "c",
    "rle.h": "c",
    "test_value.h": "c",
    "test_rle.h": "c",
    "scanner.h": "c",
    "__node_handle": "c",
    "bitset": "c",
    "deque": "c",
    "locale": "c",
    "optional": "c",
    "cstring": "c",
    "*.test": "makefile",
    "test_types.h": "c",
    "__config": "c",
    "__locale": "c",
    "atomic": "c",
    "cmath": "c",
    "complex": "c",
    "memory": "c",
    "functional": "c",
    "ios": "c",
    "mutex": "c",
    "new": "c",
    "ostream": "c",
    "queue": "c",
    "stack": "c",
    "streambuf": "c",
    "utility": "c",
    "*.inc": "c",
    "table.h": "c",
    "object.h": "c",
    "cstdlib": "c"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "xaver.clang-format",
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.proseWrap": "always",
  "prettier.printWidth": 100,
  "prettier.tabWidth": 2,
  "editor.semanticHighlighting.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "functions": "#DCDCAA",
      "keywords": "#569CD6",
      "strings": "#CE9178",
      "numbers": "#B5CEA8",
      "types": "#4EC9B0"
    }
  },
  "editor.hover.enabled": true,
  "editor.hover.above": false,
  "[c]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "clangd.arguments": [
    "--background-index",
    "--compile-commands-dir=${workspaceFolder}/c_lox",
    "--completion-style=detailed",
    "--header-insertion=never",
    "--all-scopes-completion",
    "--clang-tidy",
    "--offset-encoding=utf-16",
  ]
}